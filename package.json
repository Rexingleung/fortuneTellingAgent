{
  "name": "fortunetellingagent",
  "version": "1.0.1",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "mastra dev",
    "dev:local": "wrangler dev --local",
    "dev:remote": "wrangler dev",
    "build": "mastra build",
    "deploy": "wrangler deploy --config .mastra/output/wrangler.json",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy --env production",
    "deploy:direct": "wrangler deploy",
    "start": "mastra start",
    "cli": "node scripts/cli.mjs",
    "validate-data": "node scripts/validate-data.mjs",
    "pre-deploy": "node scripts/pre-deploy-check.mjs",
    "setup": "npm install && cp .env.example .env",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "logs": "wrangler tail",
    "logs:prod": "wrangler tail --env production"
  },
  "keywords": [
    "fortune-telling",
    "astrology",
    "tarot",
    "feng-shui",
    "numerology",
    "palmistry",
    "i-ching",
    "ba-zi",
    "divination",
    "chinese-astrology",
    "ai-agent",
    "mastra",
    "sse-streaming",
    "cloudflare-workers"
  ],
  "author": "Rexing Leung",
  "license": "ISC",
  "description": "A comprehensive AI fortune telling agent supporting multiple divination systems including astrology, tarot, I Ching, feng shui, numerology, palmistry, and Ba Zi. Optimized for Cloudflare Workers with SSE streaming support.",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^0.2.15",
    "@ai-sdk/openai": "^1.3.24",
    "@mastra/core": "^0.13.1",
    "@mastra/deployer-cloudflare": "^0.12.0",
    "@mastra/libsql": "^0.13.1",
    "@mastra/loggers": "^0.10.6",
    "@mastra/memory": "^0.12.1",
    "chalk": "^5.3.0",
    "wrangler": "^4.29.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "eslint": "^9.0.0",
    "mastra": "^0.10.20",
    "prettier": "^3.0.0",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rexingleung/fortuneTellingAgent.git"
  },
  "bugs": {
    "url": "https://github.com/Rexingleung/fortuneTellingAgent/issues"
  },
  "homepage": "https://github.com/Rexingleung/fortuneTellingAgent#readme",
  "bin": {
    "fortune-agent": "./scripts/cli.mjs"
  }
}
